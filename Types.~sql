-- below we have the necessary types for the webapp to use and 
create type emp_details_type as object(
  acc_id number,
  emp_id number,
  f_name varchar2(80),
  l_name varchar2(80),
  dep_id number,
  tm_id number,
  email varchar2(80),
  hire_date date
);

create public synonym emp_details_type for emp_details_type ;

declare 

  a emp_details_type;

begin

  emp_details.get_emp_details(p_acc_name => 'admin', p_emp_out => a);
  dbms_output.put_line(a.f_name || a.l_name);
end;
